#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/dist/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/dist/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules:/mnt/c/Users/Dell/Documents/GitHub/sokoclick/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/dist/stdio.js" "$@"
else
  exec node  "$basedir/../.pnpm/@supabase+mcp-server-supabase@0.3.5/node_modules/@supabase/mcp-server-supabase/dist/stdio.js" "$@"
fi
