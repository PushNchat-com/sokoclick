{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fast": "VITE_API_TIMEOUT=10000 vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:visual": "percy exec -- cypress run",
    "prepare": "cd .. && husky install frontend/.husky",
    "type-check": "tsc --noEmit",
    "init:slots": "tsx src/scripts/initializeSlotStorage.ts",
    "clean:vite": "node ./scripts/clear-vite-cache.js",
    "dev:clean": "npm run clean:vite && vite --force",
    "gen:types": "cd ../supabase && pnpm dlx supabase gen types typescript --project-id lcvafavhrlzqanwvrxgs --schema public > ../frontend/src/types/supabase-types.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/supabase-js": "2.39.3",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-table": "^8.21.3",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "3.3.1",
    "lucide-react": "^0.503.0",
    "react": "18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "2.0.4",
    "react-hot-toast": "2.5.2",
    "react-router-dom": "6.22.1",
    "tailwind-merge": "2.2.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.1",
    "@eslint/js": "^9.25.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "20.11.19",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@types/uuid": "9.0.8",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "10.4.17",
    "axe-core": "^4.10.3",
    "cssnano": "6.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "jest-axe": "^10.0.0",
    "postcss": "8.4.35",
    "postcss-import": "16.0.0",
    "postcss-preset-env": "9.3.0",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "typescript-eslint": "^8.31.0",
    "vite": "5.1.3",
    "vitest": "^3.1.2"
  }
}
